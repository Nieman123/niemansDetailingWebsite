{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
   "rewrites": [
      {
        "source": "/schedule",
        "destination": "/schedule.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}